package while_loop;

import java.util.Scanner;

public class while_loop {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		int number=0;
		System.out.print("Enter a number:  ");
		number = input.nextInt();
		
		while(number > 0)
		{
			System.out.println(number);
			number = number - 5;
		}
		
		input.close();

	}

}
