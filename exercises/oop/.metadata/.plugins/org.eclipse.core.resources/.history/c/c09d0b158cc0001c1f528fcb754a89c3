package exercise2;

public class Whale extends Mammal
{
	private String m_category;
	private float m_length;
	
	
	public Whale() {
		
	}
	
	public Whale(String n, float w, float a) throws NullPointerException, NegativeWeight, NegativeAge {
		super(n, w, a);
	}
	
	@Override
	public void utterVoice() {
		System.out.println("\nPUUUUUUUU");
	}

	
}
